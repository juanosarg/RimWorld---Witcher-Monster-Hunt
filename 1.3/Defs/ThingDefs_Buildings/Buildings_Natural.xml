<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	<ThingDef ParentName="BuildingBase">
		<defName>WMH_GhoulBody</defName>
		<label>ghoul victim</label>
		<description>This half-eaten corpse is utterly disgusting. A rotten stench emanates from it.</description>
		<thingClass>Building</thingClass>
		<graphicData>
			<texPath>Things/Buildings/Clutter/GhoulBody</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>(1.5,1.5)</drawSize>
		</graphicData>
		<altitudeLayer>BuildingOnTop</altitudeLayer>
		<rotatable>false</rotatable>
		<costList>
			<Leather_Human>25</Leather_Human>
			<Meat_Human>20</Meat_Human>
		</costList>
		<statBases>
			<MaxHitPoints>50</MaxHitPoints>
			<Mass>4</Mass>
			<Flammability>1.0</Flammability>
			<MarketValue>1</MarketValue>
			<WorkToBuild>1000</WorkToBuild>
		</statBases>
		<fillPercent>0.5</fillPercent>
		<destroyable>true</destroyable>
		<useHitPoints>true</useHitPoints>
		<tickerType>Normal</tickerType>
		<selectable>true</selectable>
		<size>(1,1)</size>

		<blockPlants>true</blockPlants>

	</ThingDef>
	
